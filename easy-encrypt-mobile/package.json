    "expo": "48.0.19",
    "expo-clipboard": "4.1.2",
    "expo-status-bar": "1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@types/styled-components-react-native": "5.2.1",
    "eas-cli": "3.13.2"
  },
  "private": true,
  "workspaces": {
    "nohoist": [
      "@expo/vector-icons",
      "expo"
    ]
